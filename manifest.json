{
  "manifest_version": 3,
  "name": "WeWeb CSS Variables Autocomplete",
  "version": "1.0",
  "description": "Provides autocomplete suggestions for CSS variables in WeWeb Editor",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.weweb.io/editor/*"],
      "js": ["content.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}